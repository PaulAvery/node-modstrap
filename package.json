{
  "name": "modstrap",
  "version": "2.2.2",
  "description": "Bootstrap npm modules",
  "bin": "index.js",
  "dependencies": {
    "handlebars": "^2.0.0",
    "inquirer": "^0.8.0",
    "tdir": "^1.0.1"
  },
  "author": "Florian Albertz",
  "license": "BSD-3-Clause",
  "scripts": {
    "prepublish": "make && make lint",
    "test": "make lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PaulAvery/node-modstrap.git"
  },
  "devDependencies": {
    "eslint": "^0.18.0",
    "version-bump": "^0.1.0"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "curly": [
        2,
        "multi-line"
      ],
      "quotes": [
        2,
        "single"
      ]
    },
    "ecmaFeatures": {
      "modules": true
    }
  }
}